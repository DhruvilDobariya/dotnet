{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "@fortawesome/fontawesome-free@6.2.1",
      "destination": "Content/lib/fontawesome/fontawesome-free/",
      "files": [
        "css/all.min.css",
        "js/all.min.js"
      ]
    },
    {
      "library": "bootstrap@5.2.3",
      "destination": "Content/lib/bootstrap/",
